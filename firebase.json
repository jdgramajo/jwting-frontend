{
  "hosting": {
    "public": "build/",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "redirects": [
      {
        "source": "/",
        "destination": "v1/",
        "type": "301"
      }
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "v1/index.html"
      }
    ]
  },
  "emulators": {}
}
